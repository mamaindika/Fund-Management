    <body>
       <div class="container-fluid text-center" style="background-color:#fff;background-image:url('images/Z.jpg');margin-bottom: 0px">    
           <div class="row content">
		<div class="col-md-4">
		</div>
		<div class="col-md-4" style=" padding-top:100px">
    <div class="panel panel-default">
    <div class="panel-heading" style="background-color:#FF0">
					<h3 class="panel-title" style="color:black; text-align: left; padding-left:5px">
                                            <b>Login</b>
					</h3>
				</div>
<div class="panel-body" style="background-color:#ffff99">				
<form role="form" class="form-horizontal" name="loginForm" action="login" method="post">
<div class="form-group">
<label class="col-md-4 control-label" for="User Name" style="text-align:left">User Name</label> 
<div class="col-md-6">
<input id="User Name" name="username" type="text" placeholder="username" class="form-control input-sm" required="" style="padding-left:10px">

</div>
</div>
                        <div class="form-group">
<label class="col-md-4 control-label" for="InputPassword" style="text-align:left">Password</label> 
<div class="col-md-6">
<input id="password" name="password" type="password" placeholder="password" class="form-control input-sm" required="" style="padding-left:10px">

</div>
</div>
				
<div class="form-group">
<label class="col-md-4 control-label" for="sel1" style="text-align:left">Fund Type</label>
<div class="col-md-6">
<select class="form-control input-sm" id="sel1">
    <option value="select">Select</option>
<option value="Provident Fund">Provident Fund</option>
<option value="Pension">Pension</option>
<option value="W & NOP">W & NOP</option>
<option value="Medical">Medical</option>
  </select>
</div>
</div>
<div class="form-group">
<div class="col-md-3">
<div style="padding-left:250px;">
<input type="submit" value="Login" name="loginButton">
<!-- <button id="login" type="submit" value="Login" name="loginButton" class="btn btn-sm btn-info">Login</button>

-->
</div>
</div>
</div>
    <br><br>
</form>

</div>
